    "@tanstack/react-query-devtools": "^5.32.0",
    "@tanstack/react-query-persist-client": "^5.32.0",
    "chakra-react-select": "^4.7.6",
    "dotenv": "^16.4.5",
    "formik": "^2.4.6",
    "framer-motion": "^11.1.7",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",

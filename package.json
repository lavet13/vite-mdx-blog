{
  "name": "vite-react-ssr-typescript",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx server",
    "build": "rm -rf dist && yarn run build:client && yarn run build:server && yarn run preview",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr ./entry-server.tsx",
    "preview": "cross-env NODE_ENV=production tsx --no-warnings ./server.ts",
    "theme": "chakra-cli tokens ./src/theme/index.ts",
    "theme:watch": "chakra-cli tokens ./src/theme/index.ts --watch"
  },
